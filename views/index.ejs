

<h1>See all the students!</h1>

<!-- this corresponds to the app.get function in app.js -->
<!-- each student is an object with two properties -->
<% students.forEach(function(student){ %>

	<p>
		<%= student.name %>

		<form action='/students/<%= student.id%>?_method=DELETE' method='POST'>
		  	<input type="submit" value="X">
		</form>
	</p>

<% })%>
	
	





